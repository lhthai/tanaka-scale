<!DOCTYPE html>
<html lang="vi">
<% include ../../partials/head %>

<body>
    <% include ../../partials/header %>
    <div id="page-header">
        <div class="title-breadcrumbs">
            <h1><%- i18n.__("nav_products") %></h1>
            <div class="thebreadcumb">
                <ul class="breadcrumb">
                    <li><a href="/products" target="_self"><%- i18n.__("nav_products") %></a></li>
                    <li><a href="/products/bench-scales" target="_self"><%- i18n.__("bench-scale") %></a></li>
                    <li class="active"><span><%- i18n.__("dg-1100") %></span></li>
                </ul>
            </div>
        </div>
    </div>

    <section class="services2" style="background: #E9E9E9">
        <div class="row">
            <div class="col-md-offset-1 col-md-10">
                <h2 title="TT-bX Type" class="text-left"><span><%- i18n.__("dg-1100") %></span></h2><br />
                <p class="just"><%- i18n.__("dg-1100-p") %></p>
            </div>
        </div>
    </section>

    <section class="calltoaction home3" style="background: #fff">
        <div class="row spacing-45">
            <div class="col-md-offset-1 col-sm-10">
                <h3><%- i18n.__("appearance") %></h3>
                <hr>
                <img src="/images/dg-1100-detail-1.jpg" alt="DidiCattle Scale (DGⅡ-1100)">
                <h3><%- i18n.__("features") %></h3>
                <hr>
                <ul>
                    <li><%- i18n.__("dg-1100-feature-li-1") %></li>
                    <li><%- i18n.__("dg-1100-feature-li-2") %></li>
                    <li><%- i18n.__("dg-1100-feature-li-3") %></li>
                </ul>
                <h3><%- i18n.__("specification") %></h3>
                <hr>
                <div class="p_table table_scroll">
                    <table class="table_box">
                        <thead>
                            <tr>
                                <th><%- i18n.__("model") %></th>
                                <th><%- i18n.__("dg-1100-model-p") %></th>
                            </tr>
                        </thead>
                        <tbody class="table01">

                            <tr>
                                <td><%- i18n.__("capacity") %></td>
                                <td>1,100kg</td>
                            </tr>
                            <tr>
                                <td><%- i18n.__("scale-interval") %></td>
                                <td>1kg</td>
                            </tr>
                            <tr>
                                <td><%- i18n.__("platform-dimension") %> (mm)</td>
                                <td>1,035×2,100</td>
                            </tr>
                            <tr>
                                <td><%- i18n.__("height") %> (mm)</td>
                                <td>900</td>
                            </tr>
                            <tr>
                                <td><%- i18n.__("mass") %></td>
                                <td>~230kg</td>
                            </tr>
                        </tbody>
                    </table>
                </div>
                <h3><%- i18n.__("external-form") %></h3>
                <hr>
                <img src="/images/dg-1100-detail-2.jpg" alt="DidiCattle Scale (DGⅡ-1100)">
            </div>
    </section>
    <% include ../../partials/footer %>
</body>
<% include ../../partials/scripts %>

</html>