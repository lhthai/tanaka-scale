<!DOCTYPE html>
<html lang="vi">
<% include ../../partials/head %>

<body>
    <% include ../../partials/header %>
    <div id="page-header">
        <div class="title-breadcrumbs">
            <h1><%- i18n.__("nav_products") %></h1>
            <div class="thebreadcumb">
                <ul class="breadcrumb">
                    <li><a href="/products" target="_self"><%- i18n.__("nav_products") %></a></li>
                    <li><a href="/products/floor-scales" target="_self"><%- i18n.__("floor-scale") %></a></li>
                    <li class="active"><span><%- i18n.__("tt") %></span></li>
                </ul>
            </div>
        </div>
    </div>

    <section class="services2" style="background: #E9E9E9">
        <div class="row">
            <div class="col-md-offset-1 col-md-10">
                <h2 title="TT Type" class="text-left"><span><%- i18n.__("tt") %></span></h2><br />
                <p class="just"><%- i18n.__("tt-detail-p") %></p>
            </div>
        </div>
    </section>

    <section class="calltoaction home3" style="background: #fff">
        <div class="row spacing-45">
            <div class="col-md-offset-1 col-sm-10">
                <h3><%- i18n.__("appearance") %></h3>
                <hr>
                <img src="/images/tt-detail-1.jpg" alt="TT Type">
                <h3><%- i18n.__("features") %></h3>
                <hr>
                <ul>
                    <li><%- i18n.__("tt-features-li-1") %></li>
                    <li><%- i18n.__("tt-features-li-2") %></li>

                </ul>
                <h3><%- i18n.__("specification") %></h3>
                <hr>
                <div class="p_table table_scroll">
                    <table class="table_box">
                        <thead>
                            <tr>
                                <th><%- i18n.__("model") %></th>
                                <th>TT-300</th>
                                <th>TT-600</th>
                                <th>TT-1</th>
                                <th>TT-1.5</th>
                                <th>TT-2</th>
                            </tr>
                        </thead>
                        <tbody class="table01">
                            <tr>
                                <td><%- i18n.__("capacity") %></td>
                                <td>300kg</td>
                                <td>600kg</td>
                                <td>1T</td>
                                <td>1.5T</td>
                                <td>2T</td>
                            </tr>
                            <tr>
                                <td><%- i18n.__("scale-interval") %></td>
                                <td>100g</td>
                                <td>200g</td>
                                <td>500g</td>
                                <td>500g</td>
                                <td>1kg</td>
                            </tr>
                            <tr>
                                <td><%- i18n.__("platform-dimension") %></td>
                                <td colspan="5">1,000×1,000mm　　1,200×1,200mm　　1,500×1,500mm</td>
                            </tr>
                            <tr>
                                <td><%- i18n.__("height") %></td>
                                <td colspan="5">145mm</td>
                            </tr>
                        </tbody>
                    </table>
                </div>
                <h3><%- i18n.__("external-form") %></h3>
                <hr>
                <img src="/images/tt-detail-2.jpg" alt="">
            </div>

        </div>

    </section>
    <% include ../../partials/footer %>
</body>
<% include ../../partials/scripts %>

</html>