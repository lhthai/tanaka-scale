                            <tr>
                                <td>UTT II-300-S</td>
                                <td>300kg</td>
                                <td>100g</td>
                                <td>O</td>
                                <td>O</td>
                                <td>O</td>
                                <td>O</td>
                                <td>O</td>
                                <td>700～1250 ×800～1500</td>
                                <td>35</td>
                            </tr>
                            <tr>
                                <td>UTT II-600-S</td>
                                <td>600kg</td>
                                <td>200g</td>
                                <td>-</td>
                                <td>-</td>
                                <td>O</td>
                                <td>O</td>
                                <td>O</td>
                                <td>700～1250 ×800～1500</td>
                                <td>35</td>
                            </tr>
                            <tr>
                                <td>UTT II-1200-S</td>
                                <td>1200kg</td>
                                <td>500g</td>
                                <td>-</td>
                                <td>-</td>
                                <td>O</td>
                                <td>O</td>
                                <td>O</td>
