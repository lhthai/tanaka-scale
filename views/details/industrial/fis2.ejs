<!DOCTYPE html>
<html lang="en">
<% include ../../partials/head %>

<body>
    <% include ../../partials/header %>
    <div id="page-header">
        <div class="title-breadcrumbs">
            <h1><%- i18n.__("nav_products") %></h1>
            <div class="thebreadcumb">
                <ul class="breadcrumb">
                    <li><a href="/products" target="_self"><%- i18n.__("nav_products") %></a></li>
                    <li><a href="/products/industrial-scales" target="_self"><%- i18n.__("industrial-scale") %></a></li>
                    <li class="active"><span><%- i18n.__("fis2") %></span></li>
                </ul>
            </div>
        </div>
    </div>

    <section class="services2" style="background: #E9E9E9">
        <div class="row">
            <div class="col-md-offset-1 col-md-10">
                <h2 title="Digi-Tough NEO (FIS2-150/300)" class="text-left"><span><%- i18n.__("fis2") %></span></h2><br />
                <p class="just"><%- i18n.__("fis2-p") %></p>
            </div>
        </div>
    </section>

    <section class="calltoaction home3" style="background: #fff">
        <div class="row spacing-45">
            <div class="col-md-offset-1 col-sm-10">
                <h3><%- i18n.__("appearance") %></h3>
                <hr>
                <img src="/images/fis2-detail-1.jpg" alt="Digi-Tough NEO (FIS2-150/300)">
                <h3><%- i18n.__("features") %></h3>
                <hr>
                <ul>
                    <li><%- i18n.__("fis2-feature-li-1") %></li>
                    <li><%- i18n.__("fis2-feature-li-2") %></li>
                    <li><%- i18n.__("fis2-feature-li-3") %></li>
                    <li><%- i18n.__("fis2-feature-li-4") %></li>
                    <li><%- i18n.__("fis2-feature-li-5") %></li>
                </ul>
                <h3><%- i18n.__("specification") %></h3>
                <hr>
                <div class="p_table table_scroll">
                    <table class="table_box">
                        <thead>
                            <tr>
                                <th><%- i18n.__("model") %></th>
                                <th>FIS2-150</th>
                                <th>FIS2-300</th>
                            </tr>
                        </thead>
                        <tbody class="table01">

                            <tr>
                                <td><%- i18n.__("capacity") %></td>
                                <td>150kg</td>
                                <td>300kg</td>
                            </tr>
                            <tr>
                                <td><%- i18n.__("scale-interval") %></td>
                                <td>50g</td>
                                <td>100g</td>
                            </tr>
                            <tr>
                                <td><%- i18n.__("platform-dimension") %></td>
                                <td colspan="2">520mm×415mm</td>
                            </tr>
                            <tr>
                                <td><%- i18n.__("self-weight") %></td>
                                <td colspan="2"><%- i18n.__("fis2-self-weight-p") %></td>
                            </tr>
                            <tr>
                                <td><%- i18n.__("ip-grade") %></td>
                                <td colspan="2"><%- i18n.__("fis2-ip-grade-p") %></td>
                            </tr>
                            <tr>
                                <td><%- i18n.__("pf9-tb-operating") %></td>
                                <td colspan="2">-10～40°C</td>
                            </tr>
                            <tr>
                                <td><%- i18n.__("display-device") %></td>
                                <td colspan="2"><%- i18n.__("fis2-display-device-p") %></td>
                            </tr>
                            <tr>
                                <td><%- i18n.__("display-contents") %></td>
                                <td colspan="2"><%- i18n.__("fis2-display-contents-p") %></td>
                            </tr>
                            <tr>
                                <td><%- i18n.__("display-device") %></td>
                                <td colspan="2"><%- i18n.__("fis2-display-device-p") %></td>
                            </tr>
                            <tr>
                                <td rowspan="4"><%- i18n.__("function") %></td>
                                <td colspan="2"><%- i18n.__("fis2-function-p-1") %></td>
                            </tr>
                            <tr>
                                <td colspan="2"><%- i18n.__("fis2-function-p-2") %></td>
                            </tr>
                            <tr>
                                <td colspan="2"><%- i18n.__("fis2-function-p-3") %></td>
                            </tr>
                            <tr>
                                <td colspan="2"><%- i18n.__("fis2-function-p-4") %></td>
                            </tr>
                            <tr>
                                <td><%- i18n.__("power") %></td>
                                <td colspan="2"><%- i18n.__("fis2-power-p") %></td>
                            </tr>
                            <tr>
                                <td><%- i18n.__("possible-operating-time") %></td>
                                <td colspan="2"><%- i18n.__("fis2-possible-operating-time-p") %></td>
                            </tr>
                            <tr>
                                <td><%- i18n.__("main-material") %></td>
                                <td colspan="2"><%- i18n.__("fis2-main-material-p") %></td>
                            </tr>
                            <tr>
                                <td><%- i18n.__("accessories") %></td>
                                <td colspan="2"><%- i18n.__("fis2-accessories-p") %></td>
                            </tr>
                        </tbody>
                    </table>
                </div>
                <h3><%- i18n.__("external-form") %></h3>
                <hr>
                <img src="/images/fis2-detail-2.jpg" alt="Digi-Tough NEO (FIS2-150/300)">
            </div>
    </section>
    <% include ../../partials/footer %>
</body>
<% include ../../partials/scripts %>

</html>